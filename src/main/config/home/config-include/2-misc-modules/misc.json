{
	"mime-types": {
        "tfpackage": "application/x-fascinator-package"
    },
    "database-service": {
        "derbyHome": "${fascinator.home}/database"
    },                
    "watcher": {
        "path": "${watcher.home}"
    },
    "subscriber": {
        "solr-event-log": {
            "uri": "http://localhost:${jetty.port}/solr/eventlog",
            "buffer": {
                "docLimit": "200",
                "sizeLimit": "204800",
                "timeLimit": "30"
            }
        }
    }
}