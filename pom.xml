			<groupId>com.googlecode.redbox-mint</groupId>
			<artifactId>redbox-project</artifactId>
			<type>pom</type>
			<version>1.9-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.googlecode.redbox-mint</groupId>
			<artifactId>redbox</artifactId>
			<type>pom</type>
			<version>1.9-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.googlecode.redbox-mint</groupId>
			<artifactId>redbox-config</artifactId>
			<version>1.9-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.googlecode.redbox-mint</groupId>
